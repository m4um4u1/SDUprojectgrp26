<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Presentation.GrabTrashExample">
    <children>
        <ImageView fx:id="bananaPeel" blendMode="MULTIPLY" fitHeight="52.0" fitWidth="88.0" layoutX="104.0"
                   layoutY="272.0" onMousePressed="#grab" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/bananaPeel.jpg"/>
            </image>
        </ImageView>
        <ImageView fx:id="straw" blendMode="MULTIPLY" fitHeight="69.0" fitWidth="88.0" layoutX="266.0" layoutY="95.0"
                   onMousePressed="#grab" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/straw.jpg"/>
            </image>
        </ImageView>
        <TextArea fx:id="inspect" editable="false" layoutX="388.0" layoutY="334.0" prefHeight="52.0" prefWidth="198.0">
            <cursor>
                <Cursor fx:constant="DEFAULT"/>
            </cursor>
        </TextArea>
        <Button fx:id="trashcan" layoutX="72.0" layoutY="72.0" mnemonicParsing="false" onMouseClicked="#deposit"
                text="I am trashcan"/>
        <TextField fx:id="feedback" editable="false" layoutX="274.0" layoutY="206.0" prefHeight="31.0" prefWidth="317.0"
                   visible="false">
            <cursor>
                <Cursor fx:constant="DEFAULT"/>
            </cursor>
        </TextField>
    </children>
</AnchorPane>
