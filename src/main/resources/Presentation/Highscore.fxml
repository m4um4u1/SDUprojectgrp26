<AnchorPane prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Presentation.HighscoreController">
    <children>
        <TextArea fx:id="highscoreText" editable="false" layoutX="248.0" layoutY="83.0" prefHeight="380.0"
                  prefWidth="224.0" text="Tryk 'Hent' knappen først" wrapText="true"/>
        <Text layoutX="267.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Highscores"
              textAlignment="CENTER">
            <font>
                <Font size="39.0"/>
            </font>
        </Text>
        <Text layoutX="44.0" layoutY="104.0" stroke="BLACK" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Her kan du se din score og hvor du har været sidst." wrappingWidth="160.0380859375"/>
        <Button layoutX="44.0" layoutY="227.0" mnemonicParsing="false" onAction="#handleButtonBack" text="Tilbage"/>
        <Button fx:id="buttonLoad" layoutX="124.0" layoutY="227.0" mnemonicParsing="false" onAction="#handleButtonLoad"
                text="Hent"/>
